<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abdigunawan.makeupme">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Makeupme"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:requestLegacyExternalStorage="true"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.noActionBar">
        <activity
            android:name=".ui.profile.pengaturanakun.editprofil.EditProfilActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.pengaturanakun.editpassword.EditPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.makeupme.saran.BeriMasukanActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.beritestimoni.BeriTestimoniActivity"
            android:exported="false" />

        <activity
            android:name=".ui.detail.paket.order.PaymentSuccessActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detail.paket.order.PaymentActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detail.paket.order.CustomizeOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detail.paket.detail.DetailPaketActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.detailselesai.DetailSelesaiOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.detailkonfirmasi.DetailKonfirmasiOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.order.detailbatal.DetailOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.makeupme.AboutActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detail.DetailMuaActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="false"
            android:label="@string/title_activity_main" />
        <activity
            android:name=".ui.auth.AuthActivity"
            android:exported="false" />
        <activity
            android:name=".ui.onboarding.OnBoardingActivity"
            android:exported="true" />
        <activity
            android:name=".ui.splashscreen.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>