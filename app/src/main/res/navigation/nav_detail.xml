<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/fragmentDetailMua"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_detail">

    <fragment
        android:id="@+id/fragmentDetailMua"
        android:name="com.abdigunawan.makeupme.ui.detail.DetailMuaFragment"
        tools:layout="@layout/fragment_detail_mua">
        <action
            android:id="@+id/action_fragmentDetailMua_to_fragmentDetailPaket"
            app:destination="@id/fragmentDetailPaket"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
    </fragment>

    <fragment
        android:id="@+id/fragmentDetailPaket"
        android:name="com.abdigunawan.makeupme.ui.detail.paket.DetailPaketFragment"
        tools:layout="@layout/fragment_detail_paket">
        <action
            android:id="@+id/action_fragmentDetailPaket_to_fragmentCustomizeOrder"
            app:destination="@id/fragmentCustomizeOrder"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim" />
    </fragment>

    <fragment
        android:id="@+id/fragmentPembayaran"
        android:name="com.abdigunawan.makeupme.ui.detail.paket.PaymentFragment"
        tools:layout="@layout/fragment_payment">
    </fragment>

    <fragment
        android:id="@+id/fragmentCustomizeOrder"
        android:name="com.abdigunawan.makeupme.ui.detail.paket.CustomizeOrderFragment"
        tools:layout="@layout/fragment_customize_order">
        <action
            android:id="@+id/action_fragmentCustomizeOrder_to_fragmentPembayaran"
            app:destination="@id/fragmentPembayaran"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim" />
    </fragment>



</navigation>